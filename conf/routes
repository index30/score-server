# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

#for login and logout
GET		/login						controllers.Application.login
POST	/login					controllers.Application.logAuth
GET		/page						controllers.Application.page
#GET		/admin						controllers.Application.admin
POST	/logAdmin					controllers.Application.logAdmin
POST	/logOut						controllers.Application.logOut
POST	/registAd					controllers.Application.registAd
#POST	/logScore					controllers.Application.logScore


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
